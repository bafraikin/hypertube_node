{
	"name": "hypertube",
	"version": "1.0.0",
	"description": "stream torrent with node js",
	"main": "app.js",
	"_moduleAliases": {
		"@app": "./dist/app"
	},
	"scripts": {
		"dev": "concurrently -k \"npm run build:watch\"  \"npm run start:dev\"",
		"start": "node dist/settings/app.js",
