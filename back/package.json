		"cookie-parser": "^1.4.4",
		"cookie-session": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"module-alias": "^2.2.2",
		"passport": "^0.4.1",
		"passport-42": "^1.2.6",
		"passport-local": "^1.0.0",
		"pg": "^7.17.1",
		"reflect-metadata": "^0.1.13",
		"torrent-stream": "^1.2.0",
		"typeorm": "^0.2.22",
		"typescript": "^3.7.4",
